<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
        http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd">

    <changeSet author="antonina" id="load-feature-data">
        <loadData file="feature_table.csv" tableName="feature">
            <column name="indicator_type" type="VARCHAR"/>
            <column name="is_drawable" type="BOOLEAN"/>
            <column name="years" type="INTEGER []"/>
            <column name="min_value" type="FLOAT"/>
            <column name="max_value" type="FLOAT"/>
            <column name="description" type="VARCHAR"/>
            <column name="agg_function" type="VARCHAR"/>
        </loadData>
    </changeSet>

</databaseChangeLog>