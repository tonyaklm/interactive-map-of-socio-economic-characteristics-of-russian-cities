<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
        http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd">

    <changeSet author="antonina" id="create-indexes">
        <createIndex tableName="feature" indexName="drawable_index">
            <column name="is_drawable"/>
        </createIndex>

        <createIndex tableName="feature" indexName="indicator_type_index">
            <column name="indicator_type"/>
        </createIndex>

        <createIndex tableName="data" indexName="region_id_index">
            <column name="region_id"/>
        </createIndex>

        <createIndex tableName="data" indexName="latitude_dd_index">
            <column name="latitude_dd"/>
        </createIndex>

        <createIndex tableName="data" indexName="longitude_dd_index">
            <column name="longitude_dd"/>
        </createIndex>


    </changeSet>

</databaseChangeLog>